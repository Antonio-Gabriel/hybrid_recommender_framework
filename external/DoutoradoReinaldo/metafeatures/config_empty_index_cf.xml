<?xml version='1.0'?>
<calculator>
	<global>
		<dataFile>DFILE</dataFile>
		<resourceFile>RFILE</resourceFile>
		<partitionLength>1</partitionLength>
		<outputFolder>../Metadata/OUTPUTFOLDER/</outputFolder>
		<basePath>HOME/BD/</basePath>
		<bufferSize>2097152</bufferSize>
		<useTextOutput /> 
	</global>
	<!-- CB index -->
	<process>
		<doItem />
		<doUser />
